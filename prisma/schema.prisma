generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Player {
  id        String @id @default(uuid())
  name      String
  hp        Int
  xp        Int
  caps      Int
  createdAt DateTime @default(now())
}